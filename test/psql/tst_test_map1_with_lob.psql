/**
 * Test for designation type rows_with_key with BLOB.
 *
 * @param p_count Number of rows selected.
 * @param p_blob  The BLOB.
 */
create procedure tst_test_map1_with_lob(in p_count int,
                                        in p_blob  blob)
reads sql data
-- type: map
begin
  select tst_c03
  ,      tst_c00
  from   TST_FOO2
  where  tst_c00 <= p_count
  ;
end
