{
  "name": "setbased/php-stratum-mysql",
  "description": "PhpStratum MySQL: A stored procedure loader and wrapper generator for MySQL and MariaDB",
  "keywords": [
    "PhpStratum",
    "MariaDB",
    "MySQL",
    "Stored Routines",
    "Stored Procedures",
    "Stored Functions",
    "Wrapper",
    "Loader"
  ],
  "type": "tool",
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "ext-mysqli": "*",
    "setbased/exception": "^1.0.0",
    "setbased/helper-code-store-mysql": "^2.0.0",
    "setbased/helper-code-store-php": "^2.0.0",
    "setbased/php-stratum-backend": "4.0.0",
    "setbased/php-stratum-common": "dev-master",
    "zendframework/zend-code": "^3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0.0",
    "phing/phing": "^2.0.0",
    "setbased/php-stratum": "4.0.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Stratum\\MySql\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Stratum\\MySql\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/"
  }
}
