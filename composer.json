{
  "name": "setbased/php-stratum-mysql",
  "description": "PhpStratum MySQL: A stored procedure loader and wrapper generator for MySQL and MariaDB",
  "keywords": [
    "PhpStratum",
    "MariaDB",
    "MySQL",
    "Stored Routines",
    "Stored Procedures",
    "Stored Functions",
    "Wrapper",
    "Loader"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "ext-mysqli": "*",
    "setbased/exception": "^2.1",
    "setbased/helper-code-store-mysql": "^2.1",
    "setbased/helper-code-store-php": "^2.4",
    "setbased/php-stratum-backend": "^5.2",
    "setbased/php-stratum-common": "^5.3",
    "setbased/php-stratum-middle": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4",
    "phing/phing": "^2.16",
    "setbased/php-stratum": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Stratum\\MySql\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Stratum\\MySql\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
